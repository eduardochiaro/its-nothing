{
  "name": "its-nothing",
  "version": "2.0.0",
  "description": "Watchface for pebble smartwatches inspired by NothingOS by NOTHING",
  "main": "src/main.c",
  "scripts": {
    "build": "wscript build",
    "clean": "wscript clean"
  },
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "devDependencies": {},
  "keywords": [
    "pebble",
    "watchface",
    "hours",
    "minutes"
  ],
  "author": "Eduardo Chiaro",
  "license": "MIT",
  "pebble": {
    "displayName": "It`s Nothing",
    "uuid": "d51a6924-f9cd-4f2c-bf31-6fb54ab7031c",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite",
      "emery"
    ],
    "capabilities": [
      "health",
      "configurable"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "BackgroundColor",
      "ForegroundColor",
      "Setting24H",
      "SettingShowAMPM",
      "DotThickness",
      "TopModule",
      "BottomModule"
    ],
    "resources": {
      "media": [
        {
          "type": "font",
          "name": "PIXEL_FONT_22",
          "file": "fonts/ndot-47-inspired-by-nothing.ttf"
        }
      ]
    }
  }
}